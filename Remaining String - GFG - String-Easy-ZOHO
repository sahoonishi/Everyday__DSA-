

class Solution {
    public String printString(String s, char ch, int count) {
        int no=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)==ch){
                no++;
                
            }
            if(no==count){
                  String str=s.substring(i+1);
                  return str;
               }
        }
        return "";
        
    }
}
