class Solution{
    public String removeConsecutiveCharacter(String S){
        
        int i;
        String str ="";
        str = str + S.charAt(0);
        for(i=1;i<S.length();i++)
        {
            if(S.charAt(i)!=S.charAt(i-1))
            {
                str+=S.charAt(i);
            }
        }
        return str;
        
    }
}
